<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane fx:id="registerPane" accessibleRole="TEXT" focusTraversable="true" prefHeight="400.0" prefWidth="600.0"
            style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Views.Controllers.RegisterController">
    <children>
        <Button fx:id="register_btn" layoutX="342.0" layoutY="329.0" mnemonicParsing="false" onAction="#RegisterAction"
                prefHeight="31.0" prefWidth="110.0" text="Register"/>
        <TextField fx:id="fullname_field" layoutX="143.0" layoutY="63.0" promptText="Full Name"/>
        <TextField fx:id="email_field" layoutX="426.0" layoutY="63.0" promptText="Email"/>
        <TextField fx:id="phone_field" layoutX="144.0" layoutY="119.0" promptText="Phone Number"/>
        <TextField fx:id="address_field" layoutX="428.0" layoutY="119.0" promptText="Address"/>
        <TextField fx:id="zipcode_field" layoutX="147.0" layoutY="187.0" promptText="Zip Code"/>
        <TextField fx:id="login_field" layoutX="427.0" layoutY="188.0" promptText="Login"/>
        <PasswordField fx:id="password_field" layoutX="144.0" layoutY="258.0" promptText="Password"/>
        <PasswordField fx:id="repeat_field" layoutX="429.0" layoutY="258.0" promptText="Repeat Password"/>
        <Button fx:id="back_btn" layoutX="101.0" layoutY="318.0" mnemonicParsing="false" onAction="#BackAction"
                prefHeight="31.0" prefWidth="94.0" style="-fx-background-color: transparent;">
            <graphic>
                <FontAwesomeIconView fill="#61c4f2" glyphName="BACKWARD" glyphSize="24"/>
            </graphic>
        </Button>
        <ImageView fitHeight="45.0" fitWidth="149.0" layoutX="6.0" layoutY="12.0" preserveRatio="true" smooth="true">
            <image>
                <Image url="@/Views/Resources/images/logo.png"/>
            </image>
        </ImageView>
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="14.0" layoutY="63.0" prefHeight="25.0"
               prefWidth="110.0" text="Full Name" textAlignment="CENTER">
            <effect>
                <Bloom threshold="0.0"/>
            </effect>
        </Label>
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="306.0" layoutY="63.0" prefHeight="25.0"
               prefWidth="110.0" text="Email" textAlignment="CENTER">
            <effect>
                <Bloom threshold="0.0"/>
            </effect>
        </Label>
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="17.0" layoutY="119.0" prefHeight="25.0"
               prefWidth="110.0" text="Phone Number" textAlignment="CENTER">
            <effect>
                <Bloom threshold="0.0"/>
            </effect>
        </Label>
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="306.0" layoutY="119.0" prefHeight="25.0"
               prefWidth="110.0" text="Address" textAlignment="CENTER">
            <effect>
                <Bloom threshold="0.0"/>
            </effect>
        </Label>
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="17.0" layoutY="188.0" prefHeight="25.0"
               prefWidth="110.0" text="Zip Code" textAlignment="CENTER">
            <effect>
                <Bloom threshold="0.0"/>
            </effect>
        </Label>
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="306.0" layoutY="188.0" prefHeight="25.0"
               prefWidth="110.0" text="Login" textAlignment="CENTER">
            <effect>
                <Bloom threshold="0.0"/>
            </effect>
        </Label>
        <Label accessibleRole="IMAGE_VIEW" alignment="CENTER" contentDisplay="CENTER" layoutX="17.0" layoutY="258.0"
               prefHeight="25.0" prefWidth="110.0" text="Password" textAlignment="CENTER">
            <effect>
                <Bloom threshold="0.0"/>
            </effect>
        </Label>
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="310.0" layoutY="258.0" prefHeight="25.0"
               prefWidth="110.0" text="Repeat Password">
            <effect>
                <Bloom threshold="0.0"/>
            </effect>
        </Label>
    </children>
</AnchorPane>
